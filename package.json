{
  "name": "react-build",
  "version": "1.0.0",
  "description": "React static site building boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/marciaga/react-build"
  },
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "start": "webpack-dev-server",
